<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgress">
        <attr name="roundColor" format="color"></attr>
        <attr name="roundProgressColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="progress" format="integer"></attr>
        <attr name="max" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="dropDownMenu">
        <attr name="underlineColor" format="color"/>
        <attr name="dividerColor" format="color"/>
        <attr name="textSelectdColor" format="color"/>
        <attr name="textUnSelectdColor" format="color"/>
        <attr name="menuBackgroundColor" format="color"/>
        <attr name="maskColor" format="color"/>
        <attr name="menuTextSize" format="dimension"/>
        <attr name="menuSelectedIcon" format="reference"/>
        <attr name="menuUnSelectedIcon" format="reference"/>
    </declare-styleable>
</resources>